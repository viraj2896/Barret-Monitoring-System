  function renderChart(labels, data) {
    var ctx = document.getElementById("lineChart").getContext('2d');
    var lineChart = new Chart(ctx, {
      type: 'line',
      data: {
        labels: labels,
        datasets: [{
          label: 'Room Activity',
          data: data,
          borderColor: "#00aaff",
          backgroundColor: "#b3e6ff",
          lineTension: 0.1

        }]
      },
      options: {
        animation: {
          duration: 1000,
          easing: 'linear'
        },
        scales: {
          xAxes: [{
            scaleLabel: {
              display: true,
              fontColor: '#006699',
              fontFamily: 'sans-serif',
              labelString: 'Hours'
            },
            ticks: {
              beginAtZero: true,
            }
          }],
          yAxes: [{
            scaleLabel: {
              display: true,
              fontColor: '#006699',
              fontFamily: 'sans-serif',
              labelString: 'Activity'
            },
            ticks: {
              beginAtZero: true,
            }
          }]
        }
      },
    });
  }

  function renderPeriodChart(labels, data, periodlabel) {
    var ctx = document.getElementById("lineChartForm").getContext('2d');
    var lineChart = new Chart(ctx, {
      type: 'line',
      data: {
        labels: labels,
        datasets: [{
          label: 'Room Activity',
          data: data,
          borderColor: "#c83349",
          backgroundColor: "#ddb6bc",
          lineTension: 0.1

        }]
      },
      options: {
        animation: {
          duration: 1000,
          easing: 'linear'
        },
        scales: {
          xAxes: [{
            scaleLabel: {
              display: true,
              fontColor: '#006699',
              fontFamily: 'sans-serif',
              labelString: periodlabel
            },
            ticks: {
              beginAtZero: true,
            }
          }],
          yAxes: [{
            scaleLabel: {
              display: true,
              fontColor: '#006699',
              fontFamily: 'sans-serif',
              labelString: 'Activity'
            },
            ticks: {
              beginAtZero: true,
            }
          }]
        }
      },
    });
  }

  function getChartData(room, activity, date) {
    var sensor_id = room;
    $.ajax({
      url: "https://embedded-sense-python.herokuapp.com/fetchFromDB/sensor-data",
      type: "POST",
      data: JSON.stringify({
        "sensor_ids": [
          room
        ],
        "date": date,
        "type_of_activity": activity
      }),
      dataType: "json",
      contentType: "application/json",
      success: function (jsonData) {
        var labels = [];
        var data = [];
        var jsondata = jsonData[sensor_id];
        console.log(jsondata)
        var groups = {};
        for (var i = 0; i < jsondata.length; i++) {
          var groupName = moment(jsondata[i].time).hours();
          if (!groups[groupName]) {
            groups[groupName] = [];
          }
          groups[groupName].push(jsondata[i].activity_count);
        }
        
        var last_activity = '';
        
        console.log('length',jsondata.length)
        console.log('last',jsondata[jsondata.length - 1].activity_count)
        last_activity = jsondata[jsondata.length - 1].activity_count;
        var pieBgColor = [];
        var pieHover = [];
        var pielabel = [];
        if(last_activity > 0){
          pieBgColor[0] = '#4e73df';
          pieHover[0] = '#2e59d9';
          pielabel[0] = 'Occupied';
        } else {
          pieBgColor[0] = '#1cc88a';
          pieHover[0] = '#17a673';
          pielabel[0] = 'Vacant';
        }

        jsondata = [];
        
        for (var groupName in groups) {
          jsondata.push({
            group: groupName,
            activity: groups[groupName]
          });
        }
        jsondata.map(function (d) {
          labels.push(d.group);
          var total_activity = 0;
          for (var j = 0; j < d.activity.length; j++) {
            total_activity += d.activity[j];
          }
          data.push(total_activity);
        });
        renderChart(labels, data);
        renderPieChart(pieBgColor, pieHover, pielabel);
      }
    });
  };

  function getPeriodChartData(room, activity, date) {
    var sensor_id = room;
    $.ajax({
      url: "https://embedded-sense-python.herokuapp.com/fetchFromDB/sensor-data",
      type: "POST",
      data: JSON.stringify({
        "sensor_ids": [
          room
        ],
        "type_of_activity": activity.toLowerCase(),
        "date": date
      }),
      dataType: "json",
      contentType: "application/json",
      success: function (jsonData) {
        var labels = [];
        var data = [];
        var jsondata = jsonData[sensor_id];
        console.log(jsondata)
        var groups = {};
        var periodlabel = '';
        if(activity.toLowerCase() == "daily"){
          for (var i = 0; i < jsondata.length; i++) {
          var groupName = moment(jsondata[i].time).hours();
          if (!groups[groupName]) {
            groups[groupName] = [];
          }
          groups[groupName].push(jsondata[i].activity_count);
          periodlabel = 'Hours';
        }} else if(activity.toLowerCase() == "monthly"){
          for (var i = 0; i < jsondata.length; i++) {
          var groupName = moment(jsondata[i].time).day();
          if (!groups[groupName]) {
            groups[groupName] = [];
          }
          groups[groupName].push(jsondata[i].activity_count);
          periodlabel = 'Day of Month';
        }
        } else if(activity.toLowerCase() == "yearly") {
          for (var i = 0; i < jsondata.length; i++) {
          var groupName = moment(jsondata[i].time).month();
          if (!groups[groupName]) {
            groups[groupName] = [];
          }
          groups[groupName].push(jsondata[i].activity_count);
          periodlabel = 'Month of Year';
        }
        } else {
          for (var i = 0; i < jsondata.length; i++) {
          var groupName = moment(jsondata[i].time).hours();
          if (!groups[groupName]) {
            groups[groupName] = [];
          }
          groups[groupName].push(jsondata[i].activity_count);
          periodlabel = 'Hours';
        }
        }       
        
        jsondata = [];
        
        for (var groupName in groups) {
          jsondata.push({
            group: groupName,
            activity: groups[groupName]
          });
        }
        jsondata.map(function (d) {
          labels.push(d.group);
          var total_activity = 0;
          for (var j = 0; j < d.activity.length; j++) {
            total_activity += d.activity[j];
          }
          data.push(total_activity);
        });
        renderPeriodChart(labels, data, periodlabel);
      }
    });
  };

  function renderPieChart(bgColor, hoverColor, label) {
    var ctx = document.getElementById("myPieChart");
    var myPieChart = new Chart(ctx, {
      type: 'doughnut',
      data: {
        labels: label,
        datasets: [{
          data: [100],
          backgroundColor: bgColor,
          hoverBackgroundColor: hoverColor,
          hoverBorderColor: "rgba(234, 236, 244, 1)",
        }],
      },
      options: {
        maintainAspectRatio: false,
        tooltips: {
          backgroundColor: "rgb(255,255,255)",
          bodyFontColor: "#858796",
          borderColor: '#dddfeb',
          borderWidth: 1,
          xPadding: 15,
          yPadding: 15,
          displayColors: false,
          caretPadding: 10,
        },
        legend: {
          display: false
        },
        cutoutPercentage: 80,
      },
    });
  }

  function dateHolder() {
    $(document).ready(function () {
      var format = 'ddd MMM DD YYYY HH:mm:ss [GMT]';
      
      var sensor = $("#sensor").val();
      var date =  moment(Date(), format).format('YYYY-MM-DD');
      var activity = 'daily';
      getChartData(sensor, activity, date);
      
    });
  }

  $("#submitBtn").click(function () {
    var sensor = $(this).val();
    var activity = $('#period').val();
    var date = $('#date').val();
    if(date){
      activity = '';
      getPeriodChartData(sensor, activity, date);
    } else {
      date = '';
      getPeriodChartData(sensor, activity, date);
    }
  });
